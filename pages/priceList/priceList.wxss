/* priceList.wxss */
page{
  background: #f0eff5;
  width:100%;
  height:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.box{
  width:90%;
  background: white;
  padding:30rpx 20rpx 30rpx 20rpx;
  margin-top: 15rpx;
  border-radius: 10rpx;
}
.box1{
  display: flex;
  margin-top: 10rpx;

  
}
.box-left{
  flex:1;
}
.img-dot{
  width:20rpx;
  height:20rpx;
  float:left;
  margin-top:5rpx;
}
.img-clock{
  width:20rpx;
  height:20rpx;
  float:left;
  margin-top:5rpx;
  
}
.text-left{
  margin-left: 20rpx;
  font-size:28rpx;
  color:#333333;
  float:left;
}
.box-right{
  flex:1;
}
.pay-left{
  font-size:34rpx;
  color:#333333;
  float:left;
}
.text-right{
  font-size:28rpx;
  color:#333333;
  float: right;
}
.pay-right{
  font-size:34rpx;
  color:#ffa346;
}
